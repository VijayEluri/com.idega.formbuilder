			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>workspace</name>
		<tag-class>com.idega.formbuilder.presentation.tags.FBWorkspaceTag</tag-class>
		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>styleClass</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>view</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
</taglib>