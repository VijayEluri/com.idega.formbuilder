<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>
	<navigation-rule>
		<from-view-id>/workspace/forms/</from-view-id>
		<navigation-case>
			<from-outcome>loadFormSuccess</from-outcome>
			<to-view-id>/workspace/forms/formbuilder/</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>loadCodeSuccess</from-outcome>
			<to-view-id>/workspace/forms/formbuilder/</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>loadEntriesSuccess</from-outcome>
			<to-view-id>/workspace/forms/formadmin/</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>newFormSuccess</from-outcome>
			<to-view-id>/workspace/forms/formbuilder/</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>redirectHome</from-outcome>
			<to-view-id>/workspace/forms/</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<managed-bean>
		<managed-bean-name>actionManager</managed-bean-name>
		<managed-bean-class>com.idega.formbuilder.view.ActionManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
		  	<property-name>persistenceManager</property-name>
		    <value>#{formbuilderPersistenceManager}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>formPage</managed-bean-name>
		<managed-bean-class>com.idega.formbuilder.presentation.beans.FormPage</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>formComponent</managed-bean-name>
		<managed-bean-class>com.idega.formbuilder.presentation.beans.FormComponent</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>workspace</managed-bean-name>
		<managed-bean-class>com.idega.formbuilder.presentation.beans.Workspace</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>formDocument</managed-bean-name>
		<managed-bean-class>com.idega.formbuilder.presentation.beans.FormDocument</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
	    	<property-name>appBusiness</property-name>
		    <value>#{appBusiness}</value>
		</managed-property>
		<managed-property>
		  	<property-name>persistenceManager</property-name>
		    <value>#{formbuilderPersistenceManager}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dataSources</managed-bean-name>
		<managed-bean-class>com.idega.formbuilder.presentation.beans.DataSourceList</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
  	<managed-bean>
		<managed-bean-name>palette</managed-bean-name>
		<managed-bean-class>com.idega.formbuilder.presentation.beans.Palette</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>formField</managed-bean-name>
		<managed-bean-class>com.idega.formbuilder.presentation.beans.PaletteComponent</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>localeList</managed-bean-name>
		<managed-bean-class>com.idega.formbuilder.presentation.beans.LocaleList</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<render-kit>
	     <renderer>
	          <component-family>webface</component-family>
	          <renderer-type>fb_toolbar</renderer-type>
	          <renderer-class>com.idega.formbuilder.presentation.renderkit.FBToolbarRenderer</renderer-class>
	     </renderer>
	</render-kit>
	<render-kit>
	     <renderer>
	          <component-family>formbuilder</component-family>
	          <renderer-type>fb_panel</renderer-type>
	          <renderer-class>com.idega.formbuilder.presentation.renderkit.PanelRenderer</renderer-class>
	     </renderer>
	</render-kit>
	<render-kit>
	     <renderer>
	          <component-family>formbuilder</component-family>
	          <renderer-type>fb_container</renderer-type>
	          <renderer-class>com.idega.formbuilder.presentation.renderkit.ContainerRenderer</renderer-class>
	     </renderer>
	</render-kit>
	<component>
		<component-type>InlineEdit</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBInlineEdit</component-class>
	</component>
	<component>
		<component-type>NewFormComponent</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBNewFormComponent</component-class>
	</component>
	<component>
		<component-type>FormListItem</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBFormListItem</component-class>
	</component>
	<component>
		<component-type>AdminPage</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBAdminPage</component-class>
	</component>
	<component>
		<component-type>HomePage</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBHomePage</component-class>
	</component>
	<component>
		<component-type>Button</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBButton</component-class>
	</component>
	<component>
		<component-type>ButtonArea</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBButtonArea</component-class>
	</component>
	<component>
		<component-type>FormPage</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBFormPage</component-class>
	</component>
	<component>
		<component-type>PagesPanel</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBPagesPanel</component-class>
	</component>
	<component>
		<component-type>SourceView</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBSourceView</component-class>
	</component>
	<component>
		<component-type>MenuPanel</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBMenuPanel</component-class>
	</component>
	<component>
		<component-type>Menu</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBMenu</component-class>
	</component>
	<component>
		<component-type>Workspace</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBWorkspace</component-class>
	</component>
	<component>
		<component-type>FormProperties</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBFormProperties</component-class>
	</component>
	<component>
		<component-type>PageProperties</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBPageProperties</component-class>
	</component>
	<component>
		<component-type>SelectValuesList</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBSelectValuesList</component-class>
	</component>
	<component>
		<component-type>ViewPanel</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBViewPanel</component-class>
	</component>
	<component>
		<component-type>ComponentPropertiesPanel</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBComponentPropertiesPanel</component-class>
	</component>
	<component>
		<component-type>Palette</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBPalette</component-class>
	</component>
	<component>
		<component-type>PaletteComponent</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBPaletteComponent</component-class>
	</component>
	<component>
		<component-type>DesignView</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBDesignView</component-class>
	</component>
	<component>
		<component-type>FormComponent</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBFormComponent</component-class>
	</component>
	<component>
		<component-type>FBToolbar</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBToolbar</component-class>
	</component>
	<component>
		<component-type>FBPanel</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBPanel</component-class>
	</component>
	<component>
		<component-type>FBContainer</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBContainer</component-class>
	</component>
	<component>
		<component-type>FBToolbarButton</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBToolbarButton</component-class>
	</component>
	<component>
		<component-type>FormPreview</component-type>
		<component-class>com.idega.formbuilder.presentation.components.FBFormPreview</component-class>
	</component>
</faces-config>
