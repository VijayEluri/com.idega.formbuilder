<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>
	<managed-bean>
		<managed-bean-name>workspace</managed-bean-name>
		<managed-bean-class>com.idega.formbuilder.business.Workspace</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>viewmanager</managed-bean-name>
		<managed-bean-class>com.idega.formbuilder.view.ActionManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
  	<managed-bean>
		<managed-bean-name>palette</managed-bean-name>
		<managed-bean-class>com.idega.formbuilder.view.PaletteManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>formField</managed-bean-name>
		<managed-bean-class>com.idega.formbuilder.business.FormComponent</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>formList</managed-bean-name>
		<managed-bean-class>com.idega.formbuilder.business.FormList</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>localeList</managed-bean-name>
		<managed-bean-class>com.idega.formbuilder.business.LocaleList</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<render-kit>
	     <renderer>
	          <component-family>webface</component-family>
	          <renderer-type>fb_toolbar</renderer-type>
	          <renderer-class>com.idega.formbuilder.presentation.renderkit.FBToolbarRenderer</renderer-class>
	     </renderer>
	</render-kit>
	<render-kit>
	     <renderer>
	          <component-family>formbuilder</component-family>
	          <renderer-type>fb_panel</renderer-type>
	          <renderer-class>com.idega.formbuilder.presentation.renderkit.PanelRenderer</renderer-class>
	     </renderer>
	</render-kit>
	<render-kit>
	     <renderer>
	          <component-family>formbuilder</component-family>
	          <renderer-type>fb_container</renderer-type>
	          <renderer-class>com.idega.formbuilder.presentation.renderkit.ContainerRenderer</renderer-class>
	     </renderer>
	</render-kit>
	<render-kit>
	     <renderer>
	          <component-family>formbuilder</component-family>
	          <renderer-type>fb_field</renderer-type>
	          <renderer-class>com.idega.formbuilder.presentation.renderkit.FieldRenderer</renderer-class>
	     </renderer>
	</render-kit>
	<render-kit>
	     <renderer>
	          <component-family>formbuilder</component-family>
	          <renderer-type>fb_formComponent</renderer-type>
	          <renderer-class>com.idega.formbuilder.presentation.renderkit.FormComponentRenderer</renderer-class>
	     </renderer>
	</render-kit>
	<render-kit>
	     <renderer>
	          <component-family>formbuilder</component-family>
	          <renderer-type>fb_designview</renderer-type>
	          <renderer-class>com.idega.formbuilder.presentation.renderkit.DesignViewRenderer</renderer-class>
	     </renderer>
	</render-kit>
	<render-kit>
	     <renderer>
	          <component-family>formbuilder</component-family>
	          <renderer-type>fb_paletteComponent</renderer-type>
	          <renderer-class>com.idega.formbuilder.presentation.renderkit.PaletteComponentRenderer</renderer-class>
	     </renderer>
	</render-kit>
	<render-kit>
	     <renderer>
	          <component-family>formbuilder</component-family>
	          <renderer-type>fb_palette</renderer-type>
	          <renderer-class>com.idega.formbuilder.presentation.renderkit.PaletteRenderer</renderer-class>
	     </renderer>
	</render-kit>
	<render-kit>
	     <renderer>
	          <component-family>formbuilder</component-family>
	          <renderer-type>fb_workspace</renderer-type>
	          <renderer-class>com.idega.formbuilder.presentation.renderkit.WorkspaceRenderer</renderer-class>
	     </renderer>
	</render-kit>
	<render-kit>
	     <renderer>
	          <component-family>formbuilder</component-family>
	          <renderer-type>fb_componentPropertiesPanel</renderer-type>
	          <renderer-class>com.idega.formbuilder.presentation.renderkit.ComponentPropertiesPanelRenderer</renderer-class>
	     </renderer>
	</render-kit>
	<component>
		<component-type>Workspace</component-type>
		<component-class>com.idega.formbuilder.presentation.FBWorkspace</component-class>
	</component>
	<component>
		<component-type>ComponentPropertiesPanel</component-type>
		<component-class>com.idega.formbuilder.presentation.FBComponentPropertiesPanel</component-class>
	</component>
	<component>
		<component-type>Palette</component-type>
		<component-class>com.idega.formbuilder.presentation.FBPalette</component-class>
	</component>
	<component>
		<component-type>PaletteComponent</component-type>
		<component-class>com.idega.formbuilder.presentation.FBPaletteComponent</component-class>
	</component>
	<component>
		<component-type>DesignView</component-type>
		<component-class>com.idega.formbuilder.presentation.FBDesignView</component-class>
	</component>
	<component>
		<component-type>FBFormComponent</component-type>
		<component-class>com.idega.formbuilder.presentation.FBFormComponent</component-class>
	</component>
	<component>
		<component-type>FBFormField</component-type>
		<component-class>com.idega.formbuilder.presentation.FBFormField</component-class>
	</component>
	<component>
		<component-type>FBToolbar</component-type>
		<component-class>com.idega.formbuilder.presentation.FBToolbar</component-class>
	</component>
	<component>
		<component-type>FBPanel</component-type>
		<component-class>com.idega.formbuilder.presentation.FBPanel</component-class>
	</component>
	<component>
		<component-type>FBContainer</component-type>
		<component-class>com.idega.formbuilder.presentation.FBContainer</component-class>
	</component>
	<component>
		<component-type>FBToolbarButton</component-type>
		<component-class>com.idega.formbuilder.presentation.FBToolbarButton</component-class>
	</component>
</faces-config>
